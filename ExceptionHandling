public class ExceptionHandling {
	public static void main(String args[])
	{
		int i=100,s=0;
		try {
			s=i/0;
		}
		catch(ArithmeticException e)
		{
			System.out.println(e);
		}
		finally
                {
                     System.out.println("finally block executed");
	        }
System.out.println("rest of code");

}
